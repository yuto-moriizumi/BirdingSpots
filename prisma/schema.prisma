generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL_NON_POOLING")
}

model Spot {
  id      String @id @default(uuid())
  name    String
  address String
  Jan     Int
  Feb     Int
  Mar     Int
  Apr     Int
  May     Int
  Jun     Int
  Jul     Int
  Aug     Int
  Sep     Int
  Oct     Int
  Nov     Int
  Dec     Int
}
